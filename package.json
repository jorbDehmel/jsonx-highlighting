{
  "name": "jsonx-language",
  "displayName": "JSONX Syntax Highlighting",
  "description": "VSCode language support for JSONX",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.90.0"
  },
  "icon": "logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/jorbDehmel/jsonx-highlighting.git"
  },
  "publisher": "jdehmel",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "jsonx",
      "aliases": ["jsonx", "jsonx"],
      "extensions": [".jsonx", ".jx"],
      "configuration": "./language-configuration.json",
      "icon": {"dark": "logo.png", "light": "logo.png"}
    }],
    "grammars": [{
      "language": "jsonx",
      "scopeName": "source.jsonx",
      "path": "./syntaxes/jsonx.tmLanguage.json"
    }]
  }
}
